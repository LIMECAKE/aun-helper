{
  "manifest_version": 3,
  "name": "에타츠 타이머 throttler",
  "description": "에타츠 화면을 비활성화하여도 정상적으로 사냥할 수 있도록 하는 확장프로그램입니다.",
  "version": "0.1.2",
  "permissions": [
	"storage"
  ],
  "host_permissions": [
	"https://aun.kr/"
  ],
  "content_scripts": [ {
    "all_frames": true,
    "js": ["jquery-3.6.0.min.js", "content.js"],
    "matches": ["https://aun.kr/*"],
    "run_at": "document_end"
  }],
  "web_accessible_resources": [{
	"resources": ["head.js", "config.html", "config.js", "config.css"],
	"matches": ["https://aun.kr/*"]
  }]
}